{
  "extends": "airbnb",
  "strict": "global",
  "plugins": [
    "react"
  ],

  "rules": {
    "no-param-reassign": [0, {"props": false}],
    "strict": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "max-len": [1, 120, 2, {"ignoreUrls": true}],
    "comma-dangle": 0,
    "no-mixed-operators": 1,
    "no-continue": 1
  }
}
